{
  "📜": {
    "descr": "Eigene Themen",
    "behaviour": {
      "📜 + Themenliste": "Module ignorieren und Roadmap aus Wunschthemen erstellen"
    },
    "conditions": ["👋 Start", "🗺️ Roadmap"]
  },
  "📑": {
    "descr": "Dokument",
    "behaviour": {
      "📑 + Dokument": "Module ignorieren, Dokument analysieren und daraus Roadmap erstellen"
    },
    "conditions": ["👋 Start"]
  },
  "📝": {
    "descr": "Einzelthema",
    "behaviour": {
      "📝 + Thema": "Module ignorieren und Roadmap basierend auf dem Thema erstellen"
    },
    "conditions": ["👋 Start"]
  },
  "🚀": {
    "descr": "Unterricht mit {first_topic_aus_roadmap} beginnen",
    "behaviour": { "🚀": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())" },
    "conditions": ["🗺️ Roadmap", "📝", "📜"]
  },
  "📖": {
    "descr": "Einzelthema aus PDF",
    "behaviour": { "📖": "doc.search_section(topic)" },
    "conditions": ["📑"]
  },
  "🔍": {
    "descr": "Mehr Informationen und tiefer in das Thema eintauchen",
    "behaviour": { "🔍": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())" },
    "conditions": ["⏩", "⏭️", "❓", "🧩", "🖥️", "🧮", "🗣️", "📈"]
  },
  "⏩": {
    "descr": "Unterricht fortsetzen/Weiter mit nächstem Thema",
    "behaviour": { "⏩": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())" },
    "conditions": ["IMMER (außer bei PDF)"]
  },
  "⏭️": {
    "descr": "Weiter mit Seite X aus PDF",
    "behaviour": {
      "⏭️": "doc.next_page()",
      "⏭️ + page": "doc.get_page(page)",
      "⏭️ + page(s)": "doc.get_pages(page_start, page_end)"
    },
    "conditions": ["IMMER (Bei PDF)"]
  },
  "❓": {
    "descr": "Frage",
    "behaviour": { "❓ Frage": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())" },
    "conditions": ["⏩", "⏭️", "❓", "🧩", "🖥️", "🧮", "🗣️", "📈"]
  },
  "🧩": {
    "descr": "Beispiel",
    "behaviour": { "🧩": "Musterbeispiel für Prüfung geben" },
    "conditions": ["⏩", "⏭️", "❓", "🗣️"]
  },
  "🖥️": {
    "descr": "Ausführliches Codebeispiel geben, welches das Thema vollständig abdeckt",
    "behaviour": {
      "🖥️": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())"
    },
    "conditions": ["Programmieren", "⏩", "⏭️"]
  },
  "🧮": {
    "descr": "Beispielrechnungen mit Schritt für Schritt Erklärung",
    "behaviour": {
      "🧮": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())"
    },
    "conditions": ["Mathematik", "⏩", "⏭️"]
  },
  "🗣️": {
    "descr": "Nochmal für Dummies erklären (Simple und mit vielen Beispielen)",
    "behaviour": {
      "🗣️": "setup.get_example(0).replace('### Mnemonic 🧠:', setup.random_mnemonic())"
    },
    "conditions": ["⏩", "⏭️", "❓"]
  },
  "📈": {
    "descr": "Diagramm",
    "behaviour": { "📈": "Diagramm mit Python plotten und anzeigen" },
    "conditions": ["Mathematik", "⏩", "⏭️", "❓", "🧩", "🖥️", "🧮"]
  },
  "✨": {
    "descr": "Unterricht beginnen (PDF Seite für Seite durchgehen)",
    "behaviour": { "✨": "doc.get_page(0)", "✨ + page": "doc.get_page(x)" },
    "conditions": ["📑"]
  },
  "💡": {
    "descr": "Zusammenfassung über jede Seite der PDF bekommen",
    "behaviour": { "💡": "doc.get_pdf()" },
    "conditions": ["📑"]
  }
}
